{
  "name": "@bassettsj/react-medium-editor",
  "version": "1.6.5",
  "description": "React wrapper for medium-editor",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server -c --port=8899",
    "build": "babel lib --out-dir dist && webpack -p",
    "deploy": "npm run build && github-pages-deploy",
    "test": "jest",
    "test-debug": "node --harmony $(which bugger) ./node_modules/jest-cli/bin/jest.js --runInBand"
  },
  "github-pages-deploy": {
    "src": "builds"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wangzuo/react-medium-editor.git"
  },
  "keywords": [
    "react",
    "react-component",
    "medium",
    "editor"
  ],
  "author": "Wang Zuo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wangzuo/react-medium-editor/issues"
  },
  "homepage": "https://github.com/wangzuo/react-medium-editor",
  "peerDependencies": {
    "react": ">=0.14 && <0.16"
  },
  "dependencies": {
    "blacklist": "^1.1.2",
    "medium-editor": "^5.10.0",
    "object-assign": "^4.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-jest": "^12.0.2",
    "babel-loader": "^6.1.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babel-preset-stage-1": "^6.5.0",
    "css-loader": "^0.22.0",
    "enzyme": "^2.2.0",
    "flow-bin": "^0.23.1",
    "github-pages-deploy": "0.0.3",
    "jest-cli": "^12.0.2",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "static-site-generator-webpack-plugin": "^2.0.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "react",
      "enzyme"
    ]
  }
}
